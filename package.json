    "@opentelemetry/instrumentation-restify": "0.49.0",
    "@opentelemetry/instrumentation-router": "0.48.0",
    "@opentelemetry/instrumentation-socket.io": "0.50.0",
    "@opentelemetry/instrumentation-tedious": "0.22.0",
