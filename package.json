    "change:check": "beachball check",
    "change:new": "beachball change",
    "change:bump": "beachball bump",
    "clean": "tsc --build --clean",
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json -p .",
    "compile": "tsc --build",
    "lint:fix": "eslint . --ext .ts --fix",
